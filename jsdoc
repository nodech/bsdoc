const EventEmitter = require('events').EventEmitter;
const fs = __node_require__(7 /* '/Users/matthewzipkin/Desktop/work/jsdoc/lib/jsdoc/fs' */);
const jsdoc = {
    doclet: __node_require__(141 /* '/Users/matthewzipkin/Desktop/work/jsdoc/lib/jsdoc/doclet' */),
    env: __node_require__(1 /* '/Users/matthewzipkin/Desktop/work/jsdoc/lib/jsdoc/env' */),
