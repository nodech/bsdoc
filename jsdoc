if (typeof Promise !== "undefined") old = Promise;
function noConflict() {
    try { if (Promise === bluebird) Promise = old; }
    catch (e) {}
    return bluebird;
}
var bluebird = __node_require__(21 /* './promise' */)();
bluebird.noConflict = noConflict;
module.exports = bluebird;
}],
