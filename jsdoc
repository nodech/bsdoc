[/* 161 */ 'jsdoc', '/lib/jsdoc/src/visitor.js', function(exports, module, __filename, __dirname, __meta) {
/**
 * @module jsdoc/src/visitor
 */
// TODO: consider exporting more stuff so users can override it

const jsdoc = {
    doclet: __node_require__(141 /* '/Users/matthewzipkin/Desktop/work/jsdoc/lib/jsdoc/doclet' */),
    name: __node_require__(3 /* '/Users/matthewzipkin/Desktop/work/jsdoc/lib/jsdoc/name' */),
    src: {
        astnode: __node_require__(142 /* '/Users/matthewzipkin/Desktop/work/jsdoc/lib/jsdoc/src/astnode' */),
        syntax: __node_require__(143 /* '/Users/matthewzipkin/Desktop/work/jsdoc/lib/jsdoc/src/syntax' */)
    },
    util: {
        logger: __node_require__(15 /* '/Users/matthewzipkin/Desktop/work/jsdoc/lib/jsdoc/util/logger' */)
    }
